Include /Users/jeffreypidcock/.colima/ssh_config
Include ~/.ssh/1Password/config

Host imre
	User jpdck
	HostName imre
	#SCEIcon ubuntu

Host macOS-PRL
	User jpdck
	HostName macOS.shared
	IdentityFile ~/.ssh/macos
	#SCEIcon apple

# SCE hosts that need Secretive
Host sce-* work-* *.company.com imre
	IdentityAgent ~/Library/Containers/com.maxgoedjen.Secretive.SecretAgent/Data/socket.ssh

# Default for all other hosts (1Password)
Host *
	IdentityAgent ~/Library/Group\ Containers/2BUA8C4S2C.com.1password/t/agent.sock

Host elbrus
	User jpdck
	HostName jpdck.dev
	IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
	PreferredAuthentications publickey
	PubkeyAuthentication yes
	#SCEIcon ubuntu
